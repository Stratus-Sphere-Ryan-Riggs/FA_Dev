{
  "extends": "airbnb-base/legacy",
  "env": {
    "es6": true,
    "amd": true
  },
  "globals": {
    "log": true
  },
  "rules": {
    "indent": [ 2, 4 ],
    "no-unused-vars": [
      2,
      {
        "vars": "all",
        "args": "none"
      }
    ],
    "padded-blocks": "off",
    "func-names": "off",
    "max-len": [ "error", 170 ],
    "no-trailing-spaces": ["error", {"skipBlankLines": true}]
  }
}
